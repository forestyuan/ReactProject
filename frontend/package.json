{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@redux-beacon/google-analytics-gtag": "^1.0.2",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-polyfill": "^6.26.0",
    "babelify": "^8.0.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.4.1",
    "delaunay": "^0.4.0",
    "eosjs": "^16.0.5",
    "eosjs-api": "^7.0.3",
    "eosjs-ecc": "^4.0.4",
    "file-loader": "2.0.0",
    "fs-extra": "3.0.1",
    "highcharts": "6.1.2",
    "history": "^4.7.2",
    "i18next": "^11.7.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-xhr-backend": "^1.5.1",
    "numeral": "^2.0.6",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.4.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dev-utils": "6.0.4",
    "react-dom": "^16.4.2",
    "react-i18next": "^7.11.1",
    "react-input-range": "^1.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "react-simple-dropdown": "^3.2.3",
    "react-toastify": "^4.3.1",
    "redux": "^4.0.0",
    "redux-beacon": "^2.0.3",
    "redux-persist": "^5.10.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "reduxsauce": "^1.0.0",
    "resolve": "1.6.0",
    "scatterjs-core": "^2.3.8",
    "scatterjs-plugin-eosjs": "^1.3.8",
    "seamless-immutable": "^7.1.3",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "lint": "eslint --fix --ext=js --ext=jsx ./src",
    "build": "NODE_ENV=production node scripts/build.js",
    "build-staging": "NODE_ENV=staging node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "9.1.5",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.2",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "html-webpack-plugin": "4.0.0-beta.1",
    "jest": "20.0.4",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-scss": "^2.0.0",
    "react-dev-utils-for-webpack4": "^5.0.0-webpack4",
    "redux-devtools-extension": "^2.13.5",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-manifest-plugin": "2.0.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ],
      [
        "@babel/preset-react"
      ],
      [
        "@babel/preset-flow"
      ]
    ],
    "plugins": [
      [
        "react-css-modules",
        {
          "filetypes": {
            ".scss": {
              "syntax": "postcss-scss"
            }
          },
          "exclude": "node_modules",
          "webpackHotModuleReloading": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ],
      [
        "@babel/plugin-proposal-object-rest-spread"
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
